#include <stdint.h>
#include"STM32L4xx_hal.h"

//Button = PC13 ,	BUS= AHB1
//LED = PA5 ,		BUS= AHB1




int counter ;

int main(void)
{

	//
	GPIO_InitTypeDef GPIO_initStruct={0};


	//Enabling Clock of GPIOA
	__HAL_RCC_GPIOA_IS_CLK_ENABLED();


	//Enabling Clock of GPIOA
	__HAL_RCC_GPIOC_IS_CLK_ENABLED();


	while(1){
		counter++;
	}

}
